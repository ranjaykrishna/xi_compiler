use assert

// For your sanity, since comments don't count against the the 10 lines...

main(args:int[][]) {
  a:int, b:int, c:int, d:int = foo(1,2,3,4,5,6,7,8)
  assert(a == 3)
  assert(b == 7)
  assert(c == 11)
  assert(d == 15)
}

foo(a:int, b:int, c:int, d:int, e:int, f:int, g:int, h:int):int, int, int, int{
  return a+b, c+d, e+f, g+h
}